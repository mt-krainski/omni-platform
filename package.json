{
  "name": "matts-starter",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "mkdir -p public &&npm run prepare-third-party-licenses-file && cp THIRD_PARTY_LICENSES.txt public/ && next build && node ./scripts/add-copyright-notice.js",
    "start": "next start",
    "lint": "next lint",
    "update-email-templates": "dotenv -e .env -- node scripts/update-email-templates.js",
    "prepare-third-party-licenses-file": "node ./scripts/parse-licenses.js",
    "supabase:start": "supabase start",
    "supabase:stop": "supabase stop",
    "supabase:reset": "supabase db reset",
    "supabase:push": "supabase db push",
    "supabase:pull": "supabase db pull",
    "supabase:diff": "supabase db diff --schema public",
    "supabase:types": "supabase gen types typescript --linked > src/types/database.types.ts",
    "supabase:migration:new": "supabase migration new",
    "supabase:migration:list": "supabase migration list",
    "supabase:migrate": "npm run supabase:push && npm run supabase:types",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "vitest run",
    "test:e2e": "playwright test",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.57.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.540.0",
    "next": "15.5.3",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "tailwind-merge": "^3.3.1"
  },
  "engines": {
    "node": "^22.0.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.0",
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.55.0",
    "@storybook/addon-a11y": "^9.1.1",
    "@storybook/addon-docs": "^9.1.7",
    "@storybook/addon-onboarding": "^9.1.1",
    "@storybook/addon-vitest": "^9.1.7",
    "@storybook/nextjs-vite": "^9.1.7",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^22",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "dotenv": "^17.2.2",
    "eslint": "^9",
    "eslint-config-next": "15.5.3",
    "eslint-plugin-storybook": "^9.1.1",
    "license-checker": "^25.0.1",
    "markdown-to-jsx": "^7.7.13",
    "playwright": "^1.54.2",
    "storybook": "^9.1.5",
    "supabase": "^2.33.5",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.3.8",
    "typescript": "^5",
    "vitest": "^3.2.4"
  }
}
